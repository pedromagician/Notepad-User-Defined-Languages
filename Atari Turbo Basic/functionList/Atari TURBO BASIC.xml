<NotepadPlus>
	<functionList>
			<!-- ===================================================== [ Atari TURBO BASIC ] -->
			<!-- Atari TURBO BASIC v1.0                                                      -->

			<parser
				displayName="Atari TURBO BASIC"
				id         ="Atari TURBO BASIC"
			>

				<function
					mainExpr="\s\K(proc|rem!)\x20.+?(?=\n)"
				>
					<functionName>
						<funcNameExpr expr=".*" />
					</functionName>
				</function>

			</parser>
	</functionList>
</NotepadPlus>
