<NotepadPlus>
	<functionList>
			<!-- ===================================================== [ Atari 800 TXT ] -->
			<!-- Atari 800 TXT v1.0                                                      -->

			<parser
				displayName="Atari 800 TXT"
				id         ="Atari 800 TXT"
			>

				<function
					mainExpr="\s(id=|;!)\K.+?(?=\n)"
				>
					<functionName>
						<funcNameExpr expr=".*" />
					</functionName>
				</function>

			</parser>
	</functionList>
</NotepadPlus>
